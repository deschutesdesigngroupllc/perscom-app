{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "ext-zip": "*",
    "awcodes/filament-quick-create": "^3.4.2",
    "aws/aws-sdk-php": "^3.321.6",
    "bezhansalleh/filament-shield": "^3.2.6",
    "calebporzio/sushi": "^2.5.2",
    "dutchcodingcompany/filament-socialite": "^2.2.1",
    "filament/filament": "^3.2.110",
    "filament/minimal-theme": "^3.0.7",
    "filament/spark-billing-provider": "^3.2.110",
    "filament/spatie-laravel-google-fonts-plugin": "^3.2.110",
    "filament/spatie-laravel-settings-plugin": "^3.2.110",
    "flowframe/laravel-trend": "^0.2.0",
    "guava/calendar": "^1.6",
    "guava/filament-icon-picker": "^2.2.4",
    "inertiajs/inertia-laravel": "^1.3",
    "laravel-notification-channels/discord": "^1.6",
    "laravel-notification-channels/twilio": "^3.4",
    "laravel/framework": "^11.23.5",
    "laravel/horizon": "^5.28.1",
    "laravel/octane": "^2.5.4",
    "laravel/passport": "^12.3",
    "laravel/pennant": "^1.11",
    "laravel/pulse": "^1.2.5",
    "laravel/slack-notification-channel": "^3.3.2",
    "laravel/spark-stripe": "^5.5",
    "laravel/telescope": "^5.2.2",
    "laravel/tinker": "^2.9",
    "league/flysystem-aws-s3-v3": "^3.28",
    "league/html-to-markdown": "^5.1.1",
    "owenvoke/blade-fontawesome": "^2.6",
    "parallax/filament-syntax-entry": "^2.0.1",
    "php-open-source-saver/jwt-auth": "^2.7.1",
    "pusher/pusher-php-server": "^7.2.4",
    "rlanvin/php-rrule": "^2.5.1",
    "saade/filament-fullcalendar": "^3.2.2",
    "sentry/sentry-laravel": "^4.8.0",
    "spatie/cpu-load-health-check": "^1.0.4",
    "spatie/eloquent-sortable": "^4.4",
    "spatie/fork": "^1.2.2",
    "spatie/laravel-activitylog": "^4.8",
    "spatie/laravel-backup": "dev-feature/bind-temp-service",
    "spatie/laravel-data": "^4.10.1",
    "spatie/laravel-google-fonts": "^1.4.1",
    "spatie/laravel-health": "^1.30.1",
    "spatie/laravel-permission": "^6.9",
    "spatie/laravel-responsecache": "^7.6.0",
    "spatie/laravel-settings": "^3.3.3",
    "spatie/laravel-webhook-server": "^3.8.1",
    "spatie/url": "^2.4",
    "stancl/jobpipeline": "v2.0.0-rc2 as v1.6.2",
    "stancl/tenancy": "^3.8.4",
    "stancl/virtualcolumn": "^1.4.1",
    "tailflow/laravel-orion": "^2.23.0",
    "tightenco/ziggy": "^2.3",
    "timokoerber/laravel-one-time-operations": "^1.4.2",
    "torchlight/torchlight-laravel": "^0.6.0",
    "twilio/sdk": "^7.16.2",
    "wiebenieuwenhuis/filament-code-editor": "^1.1"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3.1",
    "fakerphp/faker": "^1.23.1",
    "itsgoingd/clockwork": "^5.2.2",
    "larastan/larastan": "^2.9.8",
    "laravel/pail": "^1.1.4",
    "laravel/pint": "^1.17.3",
    "mockery/mockery": "^1.6.12",
    "nunomaduro/collision": "^8.4.0",
    "pestphp/pest": "^2.35.1",
    "pestphp/pest-plugin-laravel": "^2.4",
    "pestphp/pest-plugin-livewire": "^2.1",
    "predis/predis": "^2.2.2",
    "spatie/ignition": "^1.15",
    "spatie/laravel-ray": "^1.37.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://whizzy.dev/composer"
    },
    {
      "type": "composer",
      "url": "https://spark.laravel.com"
    },
    {
      "type": "vcs",
      "url": "https://github.com/jonerickson/laravel-backup"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-install-cmd": [
      "@php artisan filament:upgrade",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-update-cmd": [
      "@php artisan filament:upgrade",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "prepare-project": [
      "@post-root-package-install",
      "composer install",
      "npm install",
      "@post-create-project-cmd"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -k -c \"#2563eb,#d4d4d8,#4f46e5\" \"php artisan horizon\" \"php artisan pail\" \"npm run dev\" --names=queue,logs,vite"
    ],
    "docs:models": "php artisan tenants:run --tenants=1 ide-helper:models --option=reset=1",
    "shield:seeder": "php artisan tenants:run --tenants=1 shield:seeder --option=force=1",
    "lint": "./vendor/bin/pint",
    "octane": "php artisan octane:start --watch --host=perscom.test --workers=8",
    "test": "php artisan test --parallel --no-coverage --stop-on-failure --stop-on-error",
    "test:lint": "./vendor/bin/pint --test",
    "test:types": "./vendor/bin/phpstan analyse --memory-limit 1G"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
