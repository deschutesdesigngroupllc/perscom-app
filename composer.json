{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "ext-zip": "*",
    "archilex/filament-filter-sets": "^4.0.16",
    "aws/aws-sdk-php": "^3.369",
    "bezhansalleh/filament-shield": "4.0.3",
    "calebporzio/sushi": "^2.5.3",
    "dutchcodingcompany/filament-socialite": "^3.0",
    "fakerphp/faker": "^1.24",
    "filament/filament": "^4.3.1",
    "filament/spark-billing-provider": ">=4.3.1",
    "filament/spatie-laravel-google-fonts-plugin": ">=4.3.1",
    "filament/spatie-laravel-settings-plugin": ">=4.3.1",
    "flowframe/laravel-trend": "^v0.4.0",
    "guava/calendar": "^2.1.1",
    "guava/filament-icon-picker": "^3.1",
    "inertiajs/inertia-laravel": "^v2.0.16",
    "laravel-notification-channels/discord": "^1.7",
    "laravel-notification-channels/twilio": "^4.1.1",
    "laravel/framework": "^12.43.1",
    "laravel/horizon": "^5.40.2",
    "laravel/passport": "^12.4.2",
    "laravel/pennant": "^1.18.5",
    "laravel/pulse": "^1.4.6",
    "laravel/slack-notification-channel": "^3.7.0",
    "laravel/spark-stripe": "^5.8",
    "laravel/telescope": "^v5.16.0",
    "league/flysystem-aws-s3-v3": "^3.30.1",
    "league/html-to-markdown": "^5.1.1",
    "lwwcas/laravel-countries": "^4.12.0.1",
    "owenvoke/blade-fontawesome": "^2.9.1",
    "padmission/data-lens": "^2.1.3",
    "phiki/phiki": "^2.0.5",
    "php-open-source-saver/jwt-auth": "^2.8.3",
    "pusher/pusher-php-server": "^7.2.7",
    "rlanvin/php-rrule": "^2.6.0",
    "sentry/sentry-laravel": "^4.20.0",
    "spatie/cpu-load-health-check": "^1.0.5",
    "spatie/eloquent-sortable": "^4.5.2",
    "spatie/fork": "^1.2.5",
    "spatie/laravel-activitylog": "^4.10.2",
    "spatie/laravel-backup": "^9.3.6",
    "spatie/laravel-data": "^4.18.0",
    "spatie/laravel-google-fonts": "^1.4.4",
    "spatie/laravel-health": "^1.34.9",
    "spatie/laravel-permission": "^6.23.0",
    "spatie/laravel-responsecache": "^7.7.2",
    "spatie/laravel-settings": "^3.6.0",
    "spatie/laravel-webhook-server": "^3.8.3",
    "spatie/url": "^2.4",
    "stancl/jobpipeline": "v2.0.0-rc6 as v1.8.1",
    "stancl/tenancy": "^v3.9.1",
    "stancl/virtualcolumn": "^1.5.0",
    "staudenmeir/belongs-to-through": "^2.17",
    "staudenmeir/eloquent-has-many-deep": "^1.21.2",
    "staudenmeir/eloquent-json-relations": "^1.14.1",
    "symfony/expression-language": "^8.0",
    "tailflow/laravel-orion": "^2.23.0",
    "tightenco/ziggy": "^2.6.0",
    "timokoerber/laravel-one-time-operations": "^1.4.5",
    "twig/twig": "^3.22.1",
    "twilio/sdk": "^7.16.2",
    "zoha/laravel-meta": "dev-feature/publish-migrations"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.16.1",
    "barryvdh/laravel-ide-helper": "^3.6.1",
    "larastan/larastan": "^3.8.0",
    "laravel/boost": "^1.8",
    "laravel/pail": "^1.2.4",
    "laravel/pint": "^1.26.0",
    "laravel/tinker": "^2.10.2",
    "mockery/mockery": "^1.6.12",
    "nunomaduro/collision": "^8.8.3",
    "pestphp/pest": "^4.2.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "pestphp/pest-plugin-livewire": "^4.0.1",
    "rector/rector": "^2.2.14",
    "spatie/ignition": "^1.15.1",
    "spatie/laravel-ray": "^1.43.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://filamentthemes.com/composer"
    },
    {
      "type": "composer",
      "url": "https://spark.laravel.com"
    },
    {
      "type": "vcs",
      "url": "https://github.com/jonerickson/laravel-meta"
    },
    {
      "type": "composer",
      "url": "https://filament-filter-sets.composer.sh"
    },
    {
      "type": "composer",
      "url": "https://data-lens.composer.sh"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-install-cmd": [
      "@php artisan filament:upgrade",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan vendor:publish --tag=livewire:assets --ansi --force"
    ],
    "post-update-cmd": [
      "@php artisan filament:upgrade",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan vendor:publish --tag=livewire:assets --ansi --force",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "setup": [
      "composer install",
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate -n",
      "@php artisan jwt:secret -n --force",
      "@php artisan passport:keys --force",
      "npm install",
      "npm run build",
      "@php artisan perscom:install"
    ],
    "shield:seeder": "php artisan tenants:run --tenants=1 shield:seeder --option=force=1",
    "analyze": "vendor/bin/phpstan analyse --memory-limit=2G -v",
    "cs-fix": "vendor/bin/pint --parallel",
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -k -c \"#2563eb,#d4d4d8,#4f46e5\" \"php artisan horizon\" \"php artisan pail\" \"npm run dev\" --names=queue,logs,vite"
    ],
    "docs:models": [
      "@php artisan tenants:run --tenants=1 ide-helper:models --option='reset=true' --option='no-interaction=true' --option='write=true'",
      "@cs-fix app/Models"
    ],
    "ide": [
      "@php artisan tenants:run --tenants=1 ide-helper:generate",
      "@php artisan tenants:run --tenants=1 ide-helper:meta",
      "@docs:models"
    ],
    "lint": "@cs-fix",
    "rector": "./vendor/bin/rector process --ansi",
    "reset": [
      "@composer install --no-interaction --no-progress",
      "npm ci --no-audit --no-fund",
      "npm run build",
      "@php artisan migrate:fresh --seed",
      "@php artisan horizon:purge",
      "@php artisan optimize:clear",
      "@ide"
    ],
    "tc": "@test-coverage",
    "test": "@php artisan test --no-coverage --parallel",
    "test-coverage": "@php artisan test --coverage --parallel",
    "test-filter": "@php artisan test --no-coverage --filter",
    "test-lint": "@cs-fix --test",
    "test-suite": [
      "@analyze",
      "@test"
    ],
    "test-types": "@analyze",
    "test:filter": "@test-filter",
    "test:lint": "@test-lint",
    "test:suite": "@test-suite",
    "test:types": "@analyze",
    "tf": "@test-filter",
    "tl": "@test-lint",
    "ts": "@test-suite",
    "tt": "@test-types"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
