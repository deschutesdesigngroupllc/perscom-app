{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "ext-zip": "*",
    "awcodes/filament-quick-create": "^3.6.1",
    "aws/aws-sdk-php": "^3.336.8",
    "bezhansalleh/filament-shield": "^3.3.4",
    "calebporzio/sushi": "^2.5.2",
    "dutchcodingcompany/filament-socialite": "^2.3.0",
    "filament/filament": "^3.2.132",
    "filament/minimal-theme": "^3.1.2",
    "filament/spark-billing-provider": "^3.2.132",
    "filament/spatie-laravel-google-fonts-plugin": "^3.2.132",
    "filament/spatie-laravel-settings-plugin": "^3.2.132",
    "flowframe/laravel-trend": "^0.2.0",
    "guava/calendar": "^1.11",
    "guava/filament-icon-picker": "^2.2.4",
    "inertiajs/inertia-laravel": "^1.3.2",
    "laravel-notification-channels/discord": "^1.6",
    "laravel-notification-channels/twilio": "^3.4",
    "laravel/framework": "^11.41.3",
    "laravel/horizon": "^5.30.1",
    "laravel/octane": "^2.6.0",
    "laravel/passport": "^12.3.1",
    "laravel/pennant": "^1.14",
    "laravel/pulse": "^1.3.2",
    "laravel/slack-notification-channel": "^3.4.2",
    "laravel/spark-stripe": "^5.6",
    "laravel/telescope": "^5.2.6",
    "laravel/tinker": "^2.10",
    "league/flysystem-aws-s3-v3": "^3.29",
    "league/html-to-markdown": "^5.1.1",
    "owenvoke/blade-fontawesome": "^2.8",
    "parallax/filament-syntax-entry": "^2.0.1",
    "php-open-source-saver/jwt-auth": "^2.7.3",
    "pusher/pusher-php-server": "^7.2.6",
    "rlanvin/php-rrule": "^2.5.1",
    "saade/filament-fullcalendar": "^3.2.2",
    "sentry/sentry-laravel": "^4.10.2",
    "spatie/cpu-load-health-check": "^1.0.4",
    "spatie/eloquent-sortable": "^4.4.1",
    "spatie/fork": "^1.2.3",
    "spatie/laravel-activitylog": "^4.9.1",
    "spatie/laravel-backup": "dev-feature/bind-temp-service",
    "spatie/laravel-data": "^4.11.1",
    "spatie/laravel-google-fonts": "^1.4.1",
    "spatie/laravel-health": "^1.31.0",
    "spatie/laravel-permission": "^6.10.1",
    "spatie/laravel-responsecache": "^7.6.3",
    "spatie/laravel-settings": "^3.4.0",
    "spatie/laravel-webhook-server": "^3.8.2",
    "spatie/url": "^2.4",
    "stancl/jobpipeline": "v2.0.0-rc2 as v1.6.2",
    "stancl/tenancy": "^3.8.5",
    "stancl/virtualcolumn": "^1.4.1",
    "tailflow/laravel-orion": "^2.23.0",
    "tightenco/ziggy": "^2.4.2",
    "timokoerber/laravel-one-time-operations": "^1.4.4",
    "torchlight/torchlight-laravel": "^0.6.0",
    "twilio/sdk": "^7.16.2",
    "wiebenieuwenhuis/filament-code-editor": "^1.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.14",
    "barryvdh/laravel-ide-helper": "^3.4",
    "fakerphp/faker": "^1.24.1",
    "itsgoingd/clockwork": "^5.3.2",
    "larastan/larastan": "^2.9.12",
    "laravel/pail": "^1.2.1",
    "laravel/pint": "^1.19.0",
    "mockery/mockery": "^1.6.12",
    "nunomaduro/collision": "^8.5.0",
    "pestphp/pest": "^2.36.0",
    "pestphp/pest-plugin-laravel": "^2.4",
    "pestphp/pest-plugin-livewire": "^2.1",
    "predis/predis": "^2.3.0",
    "spatie/ignition": "^1.15",
    "spatie/laravel-ray": "^1.39.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://whizzy.dev/composer"
    },
    {
      "type": "composer",
      "url": "https://spark.laravel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-install-cmd": [
      "@php artisan filament:upgrade",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-update-cmd": [
      "@php artisan filament:upgrade",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "prepare-project": [
      "@post-root-package-install",
      "composer install",
      "npm install",
      "@post-create-project-cmd"
    ],
    "shield:seeder": "php artisan tenants:run --tenants=1 shield:seeder --option=force=1",
    "analyze": "vendor/bin/phpstan analyse --memory-limit=2G -v",
    "cs-fix": "vendor/bin/pint",
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -k -c \"#2563eb,#d4d4d8,#4f46e5\" \"php artisan horizon\" \"php artisan pail\" \"npm run dev\" --names=queue,logs,vite"
    ],
    "docs:models": [
      "@php artisan tenants:run --tenants=1 ide-helper:models --option='reset=true' --option='no-interaction=true' --option='write=true'",
      "@cs-fix app/Models"
    ],
    "ide": [
      "@php artisan tenants:run --tenants=1 ide-helper:generate",
      "@php artisan tenants:run --tenants=1 ide-helper:meta",
      "@docs:models"
    ],
    "lint": "@cs-fix",
    "reset": [
      "@composer install --no-interaction --no-progress",
      "npm ci --no-audit --no-fund",
      "npm run build",
      "@php artisan migrate:fresh --seed",
      "@php artisan horizon:purge",
      "@php artisan optimize:clear",
      "@ide"
    ],
    "tc": "@test-coverage",
    "test": "@php artisan test --no-coverage --parallel",
    "test-coverage": "@php artisan test --coverage --parallel",
    "test-filter": "@php artisan test --no-coverage --filter",
    "test-lint": "@cs-fix --test",
    "test-suite": [
      "@analyze",
      "@test"
    ],
    "test-types": "@analyze",
    "test:filter": "@test-filter",
    "test:lint": "@test-lint",
    "test:suite": "@test-suite",
    "test:types": "@analyze",
    "tf": "@test-filter",
    "tl": "@test-lint",
    "ts": "@test-suite",
    "tt": "@test-types"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
