{
    "name": "deschutesdesigngroupllc/perscom-3.0",
    "type": "project",
    "description": "PERSCOM Personnel Management System",
    "keywords": [
        "perscom"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.230",
        "doctrine/dbal": "^3.4",
        "dompdf/dompdf": "^2.0",
        "emilianotisato/nova-tinymce": "^2.0",
        "eminiarts/nova-tabs": "^2.2",
        "fakerphp/faker": "*",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.12",
        "laravel/nova": "^4.22",
        "laravel/passport": "^11.8",
        "laravel/pennant": "^1.2",
        "laravel/pulse": "^1.0@beta",
        "laravel/socialite": "^5.6",
        "laravel/spark-stripe": "^5.0.1",
        "laravel/telescope": "^4.12",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "openai-php/laravel": "^0.8.1",
        "outl1ne/nova-settings": "^5.0",
        "outl1ne/nova-sortable": "^3.4.2",
        "perscom/alert-card": "*",
        "perscom/calendar": "*",
        "perscom/dashboard-quick-actions": "*",
        "perscom/dashboard-title": "*",
        "perscom/document-viewer-tool": "*",
        "perscom/forms": "*",
        "perscom/html-field": "*",
        "perscom/message-field": "@dev",
        "perscom/newsfeed": "@dev",
        "perscom/roster": "*",
        "perscom/text-action-field": "@dev",
        "php-open-source-saver/jwt-auth": "^2.0",
        "rlanvin/php-rrule": "^2.4",
        "ronvanderheijden/openid-connect": "dev-feature/use-previously-declared-passport-scopes",
        "sentry/sentry-laravel": "^4.2",
        "spatie/eloquent-sortable": "^4.0.2",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-responsecache": "^7.4",
        "spatie/laravel-webhook-server": "^3.4",
        "spatie/once": "^3.1",
        "spatie/url": "^2.2",
        "stancl/jobpipeline": "dev-master as v1.6.2",
        "stancl/tenancy": "^3.7",
        "stancl/virtualcolumn": "^1.3.1",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.18",
        "tailflow/laravel-orion": "^2.19",
        "tightenco/ziggy": "^2.1",
        "timokoerber/laravel-one-time-operations": "^1.3",
        "unisharp/laravel-filemanager": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.13",
        "brianium/paratest": "^7.0",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "10.5.*",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/JonErickson/openid-connect"
        },
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://spark.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/DashboardQuickActions"
        },
        {
            "type": "path",
            "url": "./nova-components/DocumentViewerTool"
        },
        {
            "type": "path",
            "url": "./nova-components/HtmlField"
        },
        {
            "type": "path",
            "url": "./nova-components/AlertCard"
        },
        {
            "type": "path",
            "url": "./nova-components/DashboardTitle"
        },
        {
            "type": "path",
            "url": "./nova-components/Roster"
        },
        {
            "type": "path",
            "url": "./nova-components/Calendar"
        },
        {
            "type": "path",
            "url": "./nova-components/Forms"
        },
        {
            "type": "path",
            "url": "./nova-components/Newsfeed"
        },
        {
            "type": "path",
            "url": "./nova-components/MessageField"
        },
        {
            "type": "path",
            "url": "./nova-components/TextActionField"
        }
    ],
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=lfm_public --ansi --force",
            "@php artisan vendor:publish --tag=public --ansi --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "docs:models": "php artisan tenants:run ide-helper:models --option=reset=1",
        "lint": "./vendor/bin/pint",
        "test": "php artisan test --parallel --no-coverage --stop-on-failure --stop-on-error",
        "test:lint": "./vendor/bin/pint --test",
        "test:types": "./vendor/bin/phpstan analyse --memory-limit 1G"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "process-timeout": 0,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
