{
    "name": "deschutesdesigngroupllc/perscom-3.0",
    "type": "project",
    "description": "PERSCOM Personnel Management System",
    "keywords": [
        "perscom"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.230",
        "codinglabsau/laravel-feature-flags": "^1.2",
        "doctrine/dbal": "^3.4",
        "eminiarts/nova-tabs": "^2.1",
        "fakerphp/faker": "^1.9.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "innocenzi/laravel-vite": "^0.1.27",
        "kalnoy/nestedset": "^6.0",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^9.0",
        "laravel/horizon": "^5.9",
        "laravel/nova": "^4.10",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.5",
        "laravel/spark-stripe": "^1.0",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "msztorc/laravel-env": "^1.2",
        "novius/laravel-nova-order-nestedset-field": "^4.0",
        "perscom/alert-card": "*",
        "perscom/dashboard-quick-actions": "*",
        "perscom/document-viewer-tool": "*",
        "perscom/html-field": "*",
        "spatie/laravel-permission": "^5.5",
        "spatie/nova-tags-field": "^4.0",
        "spatie/url": "^2.1.0",
        "stancl/tenancy": "^3.5",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://spark.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/DashboardQuickActions"
        },
        {
            "type": "path",
            "url": "./nova-components/DocumentViewerTool"
        },
        {
            "type": "path",
            "url": "./nova-components/HtmlField"
        },
        {
            "type": "path",
            "url": "./nova-components/AlertCard"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=spark-views --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
