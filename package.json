{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "lint": "eslint --ext .js,.jsx --ignore-path .gitignore --fix resources/js",
        "format": "prettier 'resources/js/**/*.{js,jsx,json}' 'app/**/*.php'  --write --list-different --ignore-unknown --print-width 120",
        "build-dashboard-quick-actions": "cd nova-components/DashboardQuickActions && npm run dev",
        "build-dashboard-quick-actions-prod": "cd nova-components/DashboardQuickActions && npm run prod",
        "build-latest-form-submissions": "cd nova-components/LatestFormSubmissions && npm run dev",
        "build-latest-form-submissions-prod": "cd nova-components/LatestFormSubmissions && npm run prod",
        "build-resource-custom-field": "cd nova-components/ResourceCustomField && npm run dev",
        "build-resource-custom-field-prod": "cd nova-components/ResourceCustomField && npm run prod"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.11.0",
        "@inertiajs/inertia-react": "^0.8.0",
        "@inertiajs/progress": "^0.2.7",
        "react": "^17.0.0",
        "react-dom": "^17.0.0"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.17.12",
        "@headlessui/react": "^1.6.5",
        "@heroicons/react": "^1.0.6",
        "@prettier/plugin-php": "^0.18.9",
        "@tailwindcss/forms": "^0.5.2",
        "autoprefixer": "^10.4.7",
        "axios": "^0.21",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-webpack-plugin": "^3.2.0",
        "clsx": "^1.1.1",
        "focus-visible": "^5.2.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "path-browserify": "^1.0.1",
        "postcss": "^8.1.14",
        "postcss-focus-visible": "^6.0.4",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.11",
        "tailwindcss": "^3.1.3"
    }
}
